{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "food-entries": {
      "$uid": {
        ".indexOn": [
          "id",
          "name",
          "date",
          "isArchived"
        ]
      }
    },
    "meals": {
      "$uid": {
        ".indexOn": [
          "meal",
          "date"
        ]
      }
    }
  }
}